{
  "rabbit_version": "3.6.9",
  "users": [
    {
      "name": "RABBITMQ_USERNAME",
      "password": "RABBITMQ_PASSWORD",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "RABBITMQ_USERNAME",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "parameters": [],
  "policies": [],
  "exchanges": [{
    "name": "ex_logstash",
    "vhost": "/",
    "type": "direct",
    "durable": true,
    "auto_delete": false,
    "internal": false,
    "arguments": {}
  }],
  "queues": [{
    "name": "application-logs-queue",
    "vhost": "/",
    "durable": true,
    "auto_delete": false,
    "arguments": { }
  }],
  "bindings": [{
    "source": "ex_logstash",
    "vhost": "/",
    "destination": "application-logs-queue",
    "destination_type": "queue",
    "routing_key": "api-log-key",
    "arguments": {}
  }]
}